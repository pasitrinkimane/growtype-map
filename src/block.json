{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "growtype/map",
  "version": "0.1.0",
  "title": "Growtype Map",
  "category": "text",
  "icon": "flag",
  "description": "Display map in advanced way.",
  "attributes": {
    "shortcode": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "map_provider": {
      "type": "string",
      "default": "google"
    },
    "initial_latlng": {
      "type": "string",
      "default": ""
    },
    "initial_marker": {
      "type": "boolean",
      "default": true
    },
    "initial_zoom": {
      "type": "string",
      "default": ""
    },
    "map_type": {
      "type": "string",
      "default": "location"
    },
    "plain_markers": {
      "type": "string",
      "default": ""
    },
    "marker_icon_image": {
      "type": "object",
      "default": ""
    },
    "marker_icon_width": {
      "type": "string",
      "default": ""
    },
    "marker_icon_height": {
      "type": "string",
      "default": ""
    },
    "infowindow_enabled": {
      "type": "boolean",
      "default": false
    },
    "infowindow_content": {
      "type": "string",
      "default": ""
    },
    "map_height": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "growtype-map",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
